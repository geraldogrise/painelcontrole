.autocomplete-suggestion {
	border: solid 1px #ccc;
	border-top: none;
	height: auto;
	padding: 0;
	background: #fff;
	color: #777;
	font-weight: normal;
	padding: 7px 5px;
	text-transform: uppercase;
	font-size: 10px;
	cursor:default
