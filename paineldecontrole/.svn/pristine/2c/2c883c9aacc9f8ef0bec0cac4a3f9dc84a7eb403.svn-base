package br.com.util;

public class ParametroQuery {

	public ParametroQuery(String nome) {
		this.nome = nome;

	}

	public ParametroQuery(String nome, Object valor) {
		this.nome = nome;
		this.valor = valor;

	}

	private String nome = "";
	private Object valor = null;

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Object getValor() {
		return valor;
	}

	public void setValor(Object valor) {
		this.valor = valor;
	}

}
